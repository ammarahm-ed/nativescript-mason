<?xml version="1.0" encoding="utf-8"?>
<org.nativescript.mason.masonkit.View xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mason="http://schemas.android.com/apk/res-auto"
    android:id="@+id/listViewRoot"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    mason:mason_alignContent="stretch"
    mason:mason_alignItems="center"
    mason:mason_flexDirection="column">

    <TextView
        android:id="@+id/listTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Laffy Taffy!!!!" />

    <ImageView
        android:id="@+id/listImageView"
        android:layout_width="wrap_content"
        android:layout_height="50dp" />

</org.nativescript.mason.masonkit.View>
